{
  "name": "worktree-env-sync",
  "type": "module",
  "version": "2.0.7",
  "description": "CLI tool for syncing environment files across git worktrees with template interpolation and symlink support for monorepos",
  "bin": {
    "worktree-env-sync": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "worktree",
    "git-worktree",
    "env",
    "dotenv",
    "environment",
    "sync",
    "monorepo",
    "cli",
    "symlink",
    "config",
    "env-sync",
    "environment-variables",
    "claude-code",
    "cursor",
    "ai-coding",
    "copilot",
    "agentic",
    "parallel-development"
  ],
  "author": "",
  "license": "MIT",
  "scripts": {
    "-- build": "# build",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "-- lint": "# lint",
    "lint:tsc": "tsc --noEmit",
    "-- release": "# release",
    "release": "pnpm build && pnpm publish",
    "-- test": "# test",
    "test": "vitest",
    "test:watch": "vitest ",
    "-- end": "# end"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@10.20.0"
}
